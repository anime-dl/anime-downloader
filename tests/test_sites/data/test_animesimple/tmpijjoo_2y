    const iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;

    
    function resumePlayback() {
        if (iOS === true && typeof resumeAt !== 'undefined') {
            var player = videojs("player");
            setTimeout(function () {
                player.currentTime(resumeAt);
            }, 1000);
        }
    }
