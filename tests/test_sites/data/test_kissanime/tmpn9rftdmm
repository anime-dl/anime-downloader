<script type="text/javascript">


function AddHideButtonToDynamic()
{
		var elemDyna = $('div[style*="width:100%;height:768px;"]')[0];
		if (elemDyna == null)
		{
			elemDyna = $('div[style*="width: 100%; height: 768px;"]')[0];
		}
		var elemDyna2 = $('div[style*="width:100%;height:768px;"]')[1];
		if (elemDyna2 == null)
		{
			elemDyna2 = $('div[style*="width: 100%; height: 768px;"]')[1];
		}
    	if ($(elemDyna).offset() != null) {
            var topDyna = 220;
            var leftDyna = ($(window).width() - 1000) / 2 - 45;
            $(elemDyna).after('<div class="divCloseBut" style="z-index:1000;visibility:visible;position:absolute; right:' + leftDyna + 'px; top: ' + topDyna + 'px"><a href="#" onclick="$(\'#' + $(elemDyna).attr('id') + '\').remove();$(\'#' + $(elemDyna2).attr('id') + '\').remove();$(this).remove(); InitCloseButton(); return false;"><b>[Hide]</b></a></div>');
        }
}
window.setTimeout(AddHideButtonToDynamic, 5000);
</script>


        
